{
    "extends": "airbnb",
    "globals": {
        "window": true,
        "document": true,
        "fetch": true,
        "localStorage": true,
        "btoa": true
    },
    "rules": {
        "linebreak-style": 0,
         "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "react/jsx-indent-props": [
            "error",
            4
        ],
        "react/jsx-indent": [
            "error",
            4
        ]
    }
}